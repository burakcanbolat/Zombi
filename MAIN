#include "stm32f4xx.h"

void Led_Ayar()
{
	RCC->AHB1ENR = 0x00000008;

	GPIOD->MODER = 0x01000000;
	GPIOD->OTYPER = 0x00000000;
	GPIOD->OSPEEDR = 0x03000000;
	GPIOD->PUPDR = 0x00000000;
}

void Bekleme_suresi(uint32_t sure)
{
	while(sure--);
}

